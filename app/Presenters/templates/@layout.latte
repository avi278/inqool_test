

<!DOCTYPE html>
<html lang="zxx">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet">  
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <title>Iva Utíkalová</title>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Bebas+Neue&family=Poppins:wght@100;400&display=swap" rel="stylesheet">

    <link rel="stylesheet" href="{$basePath}/assets/style.css">


    <script src="{$basePath}/assets/bundle.js"></script>

</head>
<body>

	<div n:foreach="$flashes as $flash" n:class="flash, $flash->type">{$flash->message}</div>

        <header class="header" onmouseover="OverFunction()" onmouseout="OutFunction()">
            <a class="header__hamburger" onclick="dropdown_menu(); return false;" href="#"><i class="fa fa-bars"></i></a>
            <div class="header__hamburger-menu">
                <nav class="header__nav">
                    <div class="header__nav__menu">
                        <div>
                            <a onclick="openPage()" href="#portfolio">Portfolio</a>
                            <a onclick="openPage()" href="#skills">Skills</a>
                            <a onclick="openPage()" href="#contact">Contacts</a>
                        </div>
                    </div>
                </nav>
                <div class="header__search">
                    <form class="header__search__form" n:name=searchForm>
                        <input n:name=tag id="searchInput" type="text" placeholder="Search section..">
                        <button id="searchButton"  type="submit"><i class="fa fa-search"></i></button>
                    </form>
                </div>    
            </div>
    </header>

	{include content}

	{block scripts}
	<script src="https://nette.github.io/resources/js/3/netteForms.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>

	<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.14.7/dist/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>

	{/block}
</body>
</html>
